{
	"author": "Event Espresso",
	"name": "@eventespresso/icons",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eventespresso/packages.git"
	},
	"version": "1.0.5",
	"main": "src/index.tsx",
	"main:src": "src/index.tsx",
	"scripts": {
		"clean": "./scripts/cleanup.sh",
		"start": "cross-env NODE_ENV=development father doc dev --storybook",
		"lint": "eslint ./src/**/*.tsx --max-warnings=0 --format=codeframe",
		"compile": "father build",
		"test": "father test",
		"ci": "NODE_ENV=ci npm run prepublishOnly",
		"test:local": "npm run generate && npm run compile && npm run test",
		"prepublishOnly": "npm run generate && npm run compile && npm run lint && npm run test",
		"generate": "rimraf src/icons && TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=icon",
		"postcompile": "npm run clean && TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=entry"
	},
	"dependencies": {
		"@react-workspaces/react-scripts": "3.4.0-alpha-01",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-scripts": "3.4.1",
		"typescript": "~3.9.5",
		"@babel/runtime": "^7.10.1",
		"classnames": "^2.2.6",
		"insert-css": "^2.0.0",
		"rc-util": "^5.0.1"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.10.1",
		"@testing-library/react": "^10.3.0",
		"@testing-library/user-event": "^12.0.2",
		"@typescript-eslint/eslint-plugin": "^3.3.0",
		"@typescript-eslint/parser": "^3.3.0",
		"@types/classnames": "^2.2.9",
		"@types/insert-css": "^2.0.0",
		"@types/lodash": "^4.14.136",
		"@types/node": "^13.9.3",
		"@types/react": "^16.9.23",
		"@types/react-dom": "^16.9.5",
		"cross-env": "^5.2.0",
		"eslint": "^7.1.0",
		"lodash": "^4.17.15",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"rimraf": "^3.0.0",
		"styled-components": "^3.3.3",
		"ts-node": "^8.3.0",
		"typescript": "^3.0.3"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{ts,tsx}",
			"!src/setupTests.{ts,tsx}",
			"!src/serviceWorker.{ts,jsx}",
			"!src/**/*.(spec|test|stories).{ts,tsx}"
		],
		"coverageThreshold": {
			"global": {
				"statements": 98,
				"branches": 98,
				"functions": 98,
				"lines": 98
			}
		}
	},
	"license": "MIT",
	"gitHead": "66fae0230a22c100153cb05f5a624305efc5a8d9"
}
